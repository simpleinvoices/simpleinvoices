<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE REPORT SYSTEM "PHPReport.dtd">
<REPORT NUMBERFORMATEX="2">
	<PAGE CELLSPACING="0" CELLPADDING="0" ALIGN="" SIZE="2000">
      <HEADER>
	 <ROW>
            <COL CELLCLASS="si_rep_head" TEXTCLASS="BOLD">Invoice ID</COL>
            <COL CELLCLASS="si_rep_head" TEXTCLASS="BOLD">Biller</COL>
            <COL CELLCLASS="si_rep_head" TEXTCLASS="BOLD">Customer</COL>
            <COL CELLCLASS="si_rep_head" TEXTCLASS="BOLD">Total</COL>
            <COL CELLCLASS="si_rep_head" TEXTCLASS="BOLD">Paid</COL>
            <COL CELLCLASS="si_rep_head" TEXTCLASS="BOLD">Owing</COL>
         </ROW>
      </HEADER>		
      <FOOTER>
         <ROW>
            <COL COLSPAN="5" CELLCLASS="si_rep_foot">Total Owed</COL>
	    	<COL CELLCLASS="si_rep_foot si_rep_foot_total" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("inv_owing")</COL>
         </ROW>
      </FOOTER>		
   </PAGE>
	<GROUPS>
      <GROUP NAME="citygroup" >

			<FIELDS>
				<ROW>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td">index_id</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_th">biller</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_th">customer</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">inv_total</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">inv_paid</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">inv_owing</COL>
				</ROW>
			</FIELDS>
		</GROUP>
	</GROUPS>
</REPORT>
