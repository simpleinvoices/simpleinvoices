<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE REPORT SYSTEM "PHPReport.dtd">
<REPORT NUMBERFORMATEX="2">
	<PAGE CELLSPACING="0" CELLPADDING="0" ALIGN="" SIZE="2000">
      <HEADER>
         <ROW>
				  <COL	CELLCLASS="si_rep_head">id</COL>
				  <COL	CELLCLASS="si_rep_head">Biller</COL>
				  <COL	CELLCLASS="si_rep_head">Customer</COL>
				  <COL	CELLCLASS="si_rep_head">Date</COL>
				  <COL	CELLCLASS="si_rep_head">Age</COL>
				  <COL	CELLCLASS="si_rep_head">Total</COL>
				  <COL	CELLCLASS="si_rep_head">Paid</COL>
				  <COL	CELLCLASS="si_rep_head">Owing</COL>
		</ROW>
      </HEADER>		
      <FOOTER>
         <ROW>
            <COL COLSPAN="7" CELLCLASS="si_rep_foot">Total Owed</COL>
            <COL CELLCLASS="si_rep_foot si_rep_foot_total" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("inv_owing")</COL>
         </ROW>
      </FOOTER>		
   </PAGE>
	<GROUPS>
      <GROUP NAME="Aginggroup" EXPRESSION="Aging">
         <HEADER>
            <ROW>
               <COL CELLCLASS="si_rep_group_head">Aging:</COL>
               <COL CELLCLASS="si_rep_group_head" COLSPAN="7" TYPE="EXPRESSION">$this->getValue("Aging")</COL>
            </ROW>
         </HEADER>
         <FOOTER>
            <ROW>
               <COL COLSPAN="7" CELLCLASS="si_rep_group_foot">Sum</COL>
               <COL CELLCLASS="si_rep_group_foot si_rep_group_foot_total" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("inv_owing")</COL>
            </ROW>
         </FOOTER>
			<FIELDS>
				<ROW>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td">id</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_th">biller</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_th">customer</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td">date</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td">age</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">inv_total</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">inv_paid</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">inv_owing</COL>
				</ROW>
			</FIELDS>
		</GROUP>
	</GROUPS>
</REPORT>
