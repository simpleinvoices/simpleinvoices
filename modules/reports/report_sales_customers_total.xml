<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE REPORT SYSTEM "PHPReport.dtd">
<REPORT NUMBERFORMATEX="2">
   <PAGE CELLSPACING="0" CELLPADDING="0" ALIGN="">
      <HEADER>
         <ROW>
            <COL CELLCLASS="si_rep_head">Customers</COL>
            <COL CELLCLASS="si_rep_head">Amount</COL>
         </ROW>
      </HEADER>		
      <FOOTER>
         <ROW>
            <COL CELLCLASS="si_rep_foot">Total</COL>
            <COL CELLCLASS="si_rep_foot si_rep_foot_total" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("sum_total")</COL>
         </ROW>
      </FOOTER>		
   </PAGE>
	<GROUPS>
      <GROUP NAME="citygroup" >

			<FIELDS>
				<ROW>
					<COL TYPE="FIELD" CELLCLASS="si_rep_th">name</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">sum_total</COL>
				</ROW>
			</FIELDS>
		</GROUP>
	</GROUPS>
</REPORT>

