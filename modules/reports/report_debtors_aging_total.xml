<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE REPORT SYSTEM "PHPReport.dtd">
<REPORT>
	<PAGE CELLSPACING="0" CELLPADDING="0" ALIGN="" SIZE="2000">
      <HEADER>
	 	<ROW>
            <COL CELLCLASS="si_rep_head">Total</COL>
            <COL CELLCLASS="si_rep_head">Paid</COL>
            <COL CELLCLASS="si_rep_head">Owing</COL>
            <COL CELLCLASS="si_rep_head">Aging</COL>
         </ROW>
      </HEADER>		
      <FOOTER>
         <ROW>
            <COL CELLCLASS="si_rep_foot" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("inv_total")</COL>
            <COL CELLCLASS="si_rep_foot" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("inv_paid")</COL>
            <COL CELLCLASS="si_rep_foot" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("inv_owing")</COL>
            <COL CELLCLASS="si_rep_foot"></COL>
         </ROW>
      </FOOTER>		
   </PAGE>
	<GROUPS>
      <GROUP NAME="citygroup" >
			<FIELDS>
				<ROW>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">inv_total</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">inv_paid</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">inv_owing</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">aging</COL>
				</ROW>
			</FIELDS>
		</GROUP>
	</GROUPS>
</REPORT>
