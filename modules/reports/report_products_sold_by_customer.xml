<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE REPORT SYSTEM "PHPReport.dtd">
<REPORT NUMBERFORMATEX="2">
	<PAGE CELLSPACING="0" CELLPADDING="0" ALIGN="" SIZE="1000">
		<HEADER>
			<ROW>
				<COL CELLCLASS="si_rep_head">Product</COL>
				<COL CELLCLASS="si_rep_head">Quantity</COL>
			</ROW>
		</HEADER>
		<FOOTER>
			<ROW>
				<COL CELLCLASS="si_rep_foot">page total</COL>
				<COL CELLCLASS="si_rep_foot si_rep_foot_total" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("sum_quantity")</COL>
			</ROW>
		</FOOTER>
	</PAGE>
	<GROUPS>
		<GROUP NAME="citygroup" EXPRESSION="name">
			<HEADER>
				<ROW>
					<COL COLSPAN="2" CELLCLASS="si_rep_group_head" TYPE="EXPRESSION">$this->getValue("name")</COL>
				</ROW>
			</HEADER>
			<FOOTER>
				<ROW>
					<COL CELLCLASS="si_rep_group_foot">Total</COL>
					<COL CELLCLASS="si_rep_group_foot si_rep_group_foot_total" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("sum_quantity")</COL>
				</ROW>
			</FOOTER>
			<FIELDS>
				<ROW>
					<COL TYPE="FIELD" CELLCLASS="si_rep_th">description</COL>
					<COL TYPE="FIELD" CELLCLASS="si_rep_td" NUMBERFORMATEX="2">sum_quantity</COL>
				</ROW>
			</FIELDS>
		</GROUP>
	</GROUPS>
</REPORT>
